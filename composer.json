{
    "name": "behat/symfony2-extension",
    "type": "behat-extension",
    "description": "Symfony2 framework extension for Behat",
    "keywords": ["symfony", "framework", "bdd"],
    "homepage": "http://behat.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Konstantin Kudryashov",
            "email": "ever.zet@gmail.com"
        },
        {
            "name": "Christophe Coevoet",
            "email": "stof@notk.org"
        }
    ],

    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/LinioPay/Behat.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:FriendsOfBehat/Mink.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:DonCallisto/MinkExtension.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:breizh81/Mink.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:ruudk/MinkBrowserKitDriver.git"
        }
    ],

    "require": {
        "php":                       ">=7.2",
        "behat/behat":               "dev-master",
        "symfony/framework-bundle":  "~4.0|~5.0",
    },

    "require-dev": {
        "behat/mink-extension":          "dev-patch-4",
        "behat/mink":                    "dev-symfony5 as 1.7.x-dev",
        "behat/mink-browserkit-driver":  "dev-symfony-5 as ~1.3@dev",
        "phpspec/phpspec":               "~6.0",
        "phpunit/phpunit":               "~4.0|~5.0|~6.0|~7.0|~8.0"
    },

    "autoload": {
        "psr-0": { "Behat\\Symfony2Extension": "src/" }
    },

    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    }
}
